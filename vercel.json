{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    },
    {
      "src": "webapp/**",
      "use": "@vercel/static"
    },
    {
      "src": "db/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/webapp/(.*)",
      "dest": "/webapp/$1",
      "methods": ["GET"]
    },
    {
      "src": "/db/(.*)",
      "dest": "/db/$1",
      "methods": ["GET"]
    },
    {
      "src": "/(.*)",
      "dest": "index.js",
      "methods": ["POST"]
    }
  ]
}