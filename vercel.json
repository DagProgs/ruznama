{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    },
    {
      "src": "webapp/**/*",
      "use": "@vercel/static"
    },
    {
      "src": "db/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/webapp/(.*)",
      "dest": "/webapp/$1"
    },
    {
      "src": "/db/(.*)",
      "dest": "/db/$1"
    },
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ]
}