{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    },
    {
      "src": "webapp/**",
      "use": "@vercel/static"
    },
    {
      "src": "/webapp/icon.png",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1.js"
    },
    {
      "src": "/webapp",
      "dest": "/webapp/index.html"
    },
    {
      "src": "/webapp/(.*)",
      "dest": "/webapp/$1"
    },
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ]
}