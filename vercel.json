{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    },
    {
      "src": "webapp/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1.js"
    },
    {
      "src": "/webapp",
      "dest": "/webapp/index.html"
    },
    {
      "src": "/webapp/(.*)",
      "dest": "/webapp/$1"
    },
    {
      "src": "/(.*)\\.(js|css|png|jpg|jpeg|ico|svg|json|webp|woff|woff2|ttf|eot)$",
      "dest": "/webapp/$1.$2"
    },
    {
      "src": "/(.*)",
      "dest": "index.js",
      "methods": ["POST"]
    }
  ]
}